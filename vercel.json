{
    "version": 2,
    "builds": [
      {
        "src": "./package.json",  
        "use": "@vercel/static"  
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",  
        "dest": "https://backtest-omega.vercel.app/api/:1",  
        "methods": ["GET", "POST", "PUT", "DELETE"]
      },
      {
        "src": "/(.*)",  
        "dest": "./index.html"  
      }
    ]
  }