{
  "version": 2,
  "builds": [
    {
      "src": "fronttest/package.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://backtest-omega.vercel.app/api/:1",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "fronttest/index.html"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/fronttest/index.html" }
  ]
}

  
  
  
  
  
  